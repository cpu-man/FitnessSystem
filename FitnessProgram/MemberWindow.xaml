<Window x:Class="FitnessProgram.MemberWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FitnessProgram"
        mc:Ignorable="d"
        Title="MemberWindow" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="MemberBlock" HorizontalAlignment="Center" Margin="0,0,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Center"/>
        <Label Content="Medlemmer" HorizontalAlignment="Center" Margin="0,35,0,0" FontSize="20" FontWeight="Bold" VerticalAlignment="Top"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="686,35,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
</Window>